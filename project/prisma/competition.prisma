generator client {
    provider = "prisma-client-js"
    output   = "../node_modules/@prisma-app/competition"
}

datasource db {
    provider = "sqlite"
    url      = "file:db/competition.db"
}

model Round {
    id Int @id @default(autoincrement())
}

model Login {
    username       String @id @unique
    hashedPassword String
    salt           String
}
