{
  "name": "project",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "bun --bun node_modules/.bin/svelte-kit sync && bun --bun node_modules/.bin/vite dev",
    "build": "bun --bun node_modules/.bin/svelte-kit sync && bun --bun node_modules/.bin/vite build"
  },
  "devDependencies": {
    "@auth/prisma-adapter": "^2.7.4",
    "@dbushell/xml-streamify": "^0.7.0",
    "@skeletonlabs/skeleton": "^3.0.0-next.10",
    "@skeletonlabs/skeleton-svelte": "^1.0.0-next.18",
    "@sveltejs/adapter-auto": "^4.0.0",
    "@sveltejs/kit": "^2.17.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4.0.6",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.0.6",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/bun": "^1.2.2",
    "@types/node": "^22.13.1",
    "autoprefixer": "^10.4.20",
    "eslint-plugin-svelte": "^2.46.1",
    "globals": "^15.14.0",
    "prettier": "^3.5.0",
    "prettier-plugin-svelte": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "prisma": "^6.3.0",
    "sharp": "^0.33.5",
    "svelte": "^5.19.10",
    "svelte-adapter-bun": "^0.5.2",
    "svelte-check": "^4.1.4",
    "sveltekit-superforms": "^2.23.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.0",
    "vite": "^6.1.0",
    "zod": "^3.24.1"
  },
  "type": "module",
  "dependencies": {
    "@auth/sveltekit": "^1.7.4",
    "@fontsource/inter": "^5.1.1",
    "@prisma/client": "^6.3.0",
    "carta-md": "^4.6.7",
    "isomorphic-dompurify": "^2.21.0",
    "lucide-svelte": "^0.475.0"
  },
  "overrides": {
    "prisma": "6.3.0",
    "@prisma/client": "6.3.0",
    "@skeletonlabs/skeleton": "~3.0.0-next",
    "@skeletonlabs/skeleton-svelte": "~1.0.0-next",
    "tailwindcss": "^3.4.17"
  },
  "prettier": {
    "semi": false,
    "singleQuote": false,
    "tabWidth": 2,
    "printWidth": 120,
    "importOrderSeparation": true,
    "importOrder": [
      "^\\$lib\\/(.*)\\.svelte$",
      "^\\$lib\\/",
      "<THIRD_PARTY_MODULES>"
    ],
    "overrides": [
      {
        "files": "*.svelte",
        "options": {
          "parser": "svelte"
        }
      }
    ],
    "plugins": [
      "prettier-plugin-svelte",
      "prettier-plugin-tailwindcss",
      "@trivago/prettier-plugin-sort-imports"
    ]
  }
}
